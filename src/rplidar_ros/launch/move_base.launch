  <include file="$(find amcl)/examples/amcl_diff.launch" />
  <!--node pkg="rplidar_ros" type="odom_publish" name="odom_publish" /-->
  <!--

    <rosparam file="$(find rplidar_ros)/param/costmap_common_params.yaml" command="load" ns="global_costmap" />
	<rosparam file="$(find rplidar_ros)/param/costmap_common_params.yaml" command="load" ns="local_costmap" />
	<rosparam file="$(find rplidar_ros)/param/local_costmap_params.yaml" command="load" />
	<rosparam file="$(find rplidar_ros)/param/global_costmap_params.yaml" command="load" />
	<rosparam file="$(find rplidar_ros)/param/base_local_planner_params.yaml" command="load" />


  
  /-->
  <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
      
    <rosparam file="$(find rplidar_ros)/costmaps/costmap_common_params.yaml" command="load" ns="global_costmap" />
